    <servlet-class>org.heyimtaeyang.servlet.CheckUsers</servlet-class>
  </servlet>
  
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CheckCollegeName</servlet-name>
    <servlet-class>org.heyimtaeyang.servlet.CheckCollegeName</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CheckProfessionName</servlet-name>
    <servlet-class>org.heyimtaeyang.servlet.CheckProfessionName</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
