    <servlet-name>CheckAchievementName</servlet-name>
    <servlet-class>org.heyimtaeyang.servlet.CheckAchievementName</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CheckGradeTeacher</servlet-name>
    <servlet-class>org.heyimtaeyang.servlet.CheckGradeTeacher</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>DrawImage</servlet-name>
    <servlet-class>org.heyimtaeyang.servlet.DrawImage</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>ExportWordServlet.servlet</servlet-name>
    <url-pattern>/servlet/ExportWordServlet.servlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>CheckUsers</servlet-name>
    <url-pattern>/servlet/CheckUsers.servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckCollegeName</servlet-name>
    <url-pattern>/servlet/CheckCollegeName.servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckProfessionName</servlet-name>
